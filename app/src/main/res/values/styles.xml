        <item name="android:textColor">@color/white</item>
        <item name="fontFamily">@font/robotoslab_bold</item>
    </style>

    <style name="page_headings" parent="Theme.MaterialComponents">
        <item name="android:textColor">@color/white</item>
        <item name="fontFamily">@font/robotoslab_bold</item>
        <item name="android:textSize">30sp</item>
        <item name="android:textAllCaps">true</item>
    </style>

    <style name="myCustomStyle2" parent="Theme.MaterialComponents">
        <item name="android:textSize">25sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:fontFamily">@font/robotoslab_bold</item>
    </style>

    <style name="profile_page_text" parent="Theme.MaterialComponents">
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">@color/white</item>
        <item name="fontFamily">@font/robotoslab_medium</item>
    </style>
